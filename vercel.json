{
    "version": 1,
    "builds": [
        {
            "src": "minipos/wsgi.py",
            "use": "@vercel/python",
            "config": {"maxLambdaSize": "15mb", "runtime": "python3.9"}
        },
        {
            "src": "buils_files.sh",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "static"
            }
        }
    ],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/static/$1"
        },
        {
            "src": "/(.*)",
            "dest": "minipos/wsgi.py"
        }
    ]

}